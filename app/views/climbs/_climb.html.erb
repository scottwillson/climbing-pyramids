<tr>
  <td><%= climb.climbed_on.to_s(:long) %></td>
  <td><%= climb.grade.name %></td>
  <td><%= climb.discipline.name %></td>
  <td>
    <% if climb.mountain_project_route_id.present? %>
      <%= link_to climb.name, "https://www.mountainproject.com/route/#{climb.mountain_project_route_id}" %>
    <% else %>
      <%= climb.name %>
    <% end %>
  </td>
  <td><%= climb.mountain_project_type %></td>
  <td><%= climb.mountain_project_lead_style %></td>
  <td><%= climb.mountain_project_style %></td>
  <td><%= climb.mountain_project_notes %></td>
</tr>
