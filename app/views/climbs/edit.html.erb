<%= bootstrap_form_for(@climb) do |f| %>
  <%= f.text_field :name %>
  <%= f.collection_select :discipline_id, Discipline.all.sort_by(&:name), "id", "name", include_blank: true %>
  <%= f.collection_select :grade, Grade.all, "name", "name", include_blank: true, selected: @climb.grade&.name %>
  <%= f.text_field :mountain_project_tick_id %>
  <%= f.text_field :mountain_project_route_id %>
  <%= f.text_field :mountain_project_pitches %>
  <%= f.text_field :mountain_project_user_stars %>
  <%= f.text_field :mountain_project_lead_style %>
  <%= f.text_field :mountain_project_notes %>
  <%= f.text_field :mountain_project_style %>
  <%= f.text_field :mountain_project_type %>
  <%= f.text_field :mountain_project_user_rating %>
  <%= f.date_field :climbed_on %>
  <%= f.static_control :created_at %>
  <%= f.static_control :updated_at %>
  <%= f.submit "Save" %>
<% end %>
